<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.genepoint.lbsshow.dao.APMonitorMapper">
	<resultMap type="com.genepoint.lbsshow.model.APMonitor" id="apMonitorResultMap">
		<result column="apmac" property="apmac" jdbcType="VARCHAR"/>
		<result column="date" property="date" jdbcType="DATE"/>
		<result column="building" property="building" jdbcType="VARCHAR"/>
		<result column="floor" property="floor" jdbcType="VARCHAR"/>
		<result column="datas" property="datas" jdbcType="VARCHAR"/>
	</resultMap>
	
	<select id="getAPMonitor" parameterType="com.genepoint.lbsshow.model.APMonitor" resultMap="apMonitorResultMap">
		select apmac,datas from ap_monitor where building = #{building} and floor = #{floor} and date = #{date}
	</select>
</mapper>