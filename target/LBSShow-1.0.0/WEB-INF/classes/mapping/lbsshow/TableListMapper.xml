<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.genepoint.lbsshow.dao.TableListMapper" >
	<resultMap type="com.genepoint.lbsshow.model.Table" id="TableListResultMap">
		<result column="table_name" property="tableName" jdbcType="VARCHAR"/>
		<result column="add_time" property="addTime" jdbcType="VARCHAR"/>
	</resultMap>
	
	<select id="getTableList" parameterType="com.genepoint.lbsshow.model.vo.TableVO" resultMap="TableListResultMap">
		select `table_name` from table_list where left(`table_name`,25) = #{tablePreFix} and right(`table_name`,8) &gt;= #{startTime}
		and right(`table_name`,8) &lt;= #{endTime} 
	</select>
	
</mapper>