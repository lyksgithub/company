<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.genepoint.lbsshow.dao.FlowByHourMapper" >
	<resultMap type="com.genepoint.lbsshow.model.FlowByHour" id="FlowByHourResultMap">
		<id column="id" property="id" jdbcType="INTEGER"/>
		<result column="building" property="building" jdbcType="VARCHAR"/>
		<result column="time_start" property="timeStart" jdbcType="BIGINT"/>
		<result column="time_end" property="timeEnd" jdbcType="BIGINT"/>
		<result column="value" property="value" jdbcType="INTEGER"/>
		<result column="macs" property="macs" jdbcType="VARCHAR"/>
	</resultMap>
	
	<select id="getHistoryCustomer" resultMap="FlowByHourResultMap" parameterType="com.genepoint.lbsshow.model.FlowByHour">
		select value,time_end from flow_by_hour 
		where building = #{building} and time_start between #{timeStart} and #{timeEnd} order by time_start
	</select>
	
	
</mapper>